{
    "name": "JSY COVID Active Cases",
    "description": "Shows the latest COVID active cases Data for Jersey",
    "version": "1.4.5",
    "manifest_version": 3,
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png"
      },
    "action": {
        "default_popup": "index.html"
      },  
      "background": {
        "service_worker": "background.js"
      },
      "permissions": ["alarms"]
  }